                <td>@p.DezProposta.ToString("#,##0.00")</td>
                <td>@p.DezRealizado.ToString("#,##0.00")</td>
            </tr>
                }
                <tfoot style="border: solid; border-width:1.5px;">
                    @foreach (var t in valorTotal)
                    {
                        <tr>
                            <td colspan="2" class="total"><b>Total</b></td>
                            <td>@t.TotalPropJaneiro.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiJaneiro.ToString("#,##0.00")</td>
                            <td>@t.TotalPropFevereiro.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiFevereiro.ToString("#,##0.00")</td>
                            <td>@t.TotalPropMarco.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiMarco.ToString("#,##0.00")</td>
                            <td>@t.TotalPropAbril.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiAbril.ToString("#,##0.00")</td>
                            <td>@t.TotalPropMaio.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiMaio.ToString("#,##0.00")</td>
                            <td>@t.TotalPropJunho.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiJunho.ToString("#,##0.00")</td>
                            <td>@t.TotalPropJulho.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiJulho.ToString("#,##0.00")</td>
                            <td>@t.TotalPropAgosto.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiAgosto.ToString("#,##0.00")</td>
                            <td>@t.TotalPropSetembro.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiSetembro.ToString("#,##0.00")</td>
                            <td>@t.TotalPropOutubro.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiOutubro.ToString("#,##0.00")</td>
                            <td>@t.TotalPropNovembro.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiNovembro.ToString("#,##0.00")</td>
                            <td>@t.TotalPropDezembro.ToString("#,##0.00")</td>
                            <td>@t.TotalRealiDezembro.ToString("#,##0.00")</td>
                        </tr>
                    }
                </tfoot>
            </table>
            <br />
            <div class="box">
                <table class="table-striped">
                    <thead>
                        <tr>
                            <th colspan="3">Resultado</th>
                        </tr>
                        <tr>
                            <td></td>
                            <td>Proposta Total</td>
                            <td>Realizado Total</td>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var fatR in FaturamentoResult)
                        {
                            <tr>
                                <td>@fatR.Categoria</td>
                                <td>@fatR.FaturamentoPropResult.ToString("R$: #,##0.00") </td>
                                <td>@fatR.FaturamentoRealiResult.ToString("R$: #,##0.00")</td>
                            </tr>
                        }
