    md_Planilha.mdJaneiro sobre = ViewBag.sobre;
    ViewBag.Title = "Planilha";
    var a = 0;
    Layout = "~/Views/Shared/_Render.cshtml";
}

<div class="container">
    <div class="col-md-6">
        <h2>Planilha</h2>
    </div>
        <div class="col-md-6">
           <a href="/planilha/impressao" class="btn btn-default" target="_blank">Página de Impressão</a>
        </div>
        <div class="tabela">
            <table class="table-round-corner table table-striped">
                <thead>
                    <tr>

